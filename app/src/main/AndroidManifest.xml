<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.eatby">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.EatBy"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".review_class.ReviewList"></activity>
        <activity android:name=".userFav.UserFav" />
        <activity android:name=".restaurants.Reviews" />
        <activity android:name=".restaurants.MainActivity" />
        <activity android:name=".user_list.UserList" />
        <activity android:name=".ui.login.Register" />
        <activity
            android:name=".Home.Home"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.EatBy.NoActionBar" />
        <activity android:name=".apiCall.MainActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>